{
  "name": "starter",
  "version": "1.0.0",
  "description": "UI for chromolab website",
  "scripts": {
    "postinstall": "npm run modernizr",
    "styles": "node tasks/npm/styles",
    "templates": "node tasks/npm/templates",
    "scripts": "webpack",
    "fonts": "node tasks/fonts",
    "watch": "node tasks/watch",
    "favicon": "node tasks/favicons",
    "modernizr": "modernizr -c .modernizr.json -d build/",
    "build": "npm run fonts && npm run favicon && npm run modernizr && npm run build:light",
    "build:light": "npm run styles && npm run templates && npm run scripts",
    "test": "ava test/*.js",
    "posttest": "npm run lint",
    "lint": "npm run lint:scripts && npm run lint:html && npm run lint:styles",
    "lint:scripts": "eslint '{modules,src,tasks,test}/**/*.js'",
    "lint:styles": "stylelint '{modules,src}/**/*.scss'",
    "lint:html": "htmlhint '{modules,src,build}/**/*.html'",
    "deploy": "npm run build:light && npm run gh-pages",
    "gh-pages": "gh-pages -d build",
    "travis": "npm run test && npm run gh-pages",
    "surge": "surge ./build chromolab.project.alanev.ru"
  },
  "keywords": [
    "spa",
    "react"
  ],
  "author": "alanev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chromolab/chromolab-ui.git"
  },
  "bugs": {
    "url": "https://github.com/chromolab/chromolab-ui/issues"
  },
  "homepage": "https://github.com/chromolab/chromolab-ui#readme",
  "devDependencies": {
    "ava": "^0.18.1",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "browser-sync": "^2.18.6",
    "chokidar": "^1.6.1",
    "css-mqpacker": "^5.0.1",
    "cssnano": "^3.10.0",
    "del": "^2.2.2",
    "eslint": "^3.13.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-react": "^6.9.0",
    "favicons": "^4.8.3",
    "gh-pages": "^0.12.0",
    "glob": "^7.1.1",
    "htmlhint": "^0.9.13",
    "json-loader": "^0.5.4",
    "mkdirp": "^0.5.1",
    "modernizr": "^3.3.1",
    "postcss": "^5.2.9",
    "postcss-cssnext": "^2.9.0",
    "postcss-each": "^0.9.3",
    "postcss-extend": "^1.0.5",
    "postcss-focus": "^1.0.0",
    "postcss-import": "^9.0.0",
    "postcss-mixins": "^5.4.1",
    "postcss-nested": "^1.0.0",
    "postcss-property-shorthand": "^1.0.0",
    "postcss-ref": "^1.2.0",
    "postcss-scss": "^0.4.0",
    "postcss-short": "^3.0.3",
    "postcss-simple-vars": "^3.0.0",
    "postxml": "^0.2.9",
    "postxml-pack-alanev": "^1.2.0",
    "stylelint": "^7.8.0",
    "stylelint-config-standard": "^16.0.0",
    "svg-react-loader": "^0.3.7",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.16.1",
    "webpcss": "^1.1.3"
  },
  "engines": {
    "node": ">=0.6.0"
  },
  "os": [
    "linux"
  ],
  "dependencies": {
    "load-google-maps-api": "0.0.2",
    "lodash.debounce": "^4.0.8",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-images": "^0.5.2",
    "react-input-mask": "^0.7.5",
    "react-router": "^3.0.2"
  }
}
